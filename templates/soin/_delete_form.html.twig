<form method="post" action="{{ path('soin_delete', {'id': soin.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ soin.id) }}">
    <button class="btn">Delete</button>
</form>
